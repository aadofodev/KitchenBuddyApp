{
  "expo": {
    "name": "KitchenBuddyV2",
    "slug": "KitchenBuddyV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.aadofodev.KitchenBuddyV2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "kitchenbuddyv2",
    "plugins": [ 
      "expo-camera"
    ],
    "extra": { 
      "eas": {
        "projectId": "dddfdf32-1a86-47a4-81f2-6b0e4dff8656"
      }
    }
  }
}